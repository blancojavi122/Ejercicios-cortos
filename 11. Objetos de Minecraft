public class ObjetosdeMinecraft  {
    public static void main (String[] args) {
        String objetos[] = new String  [5];
        objetos[0] = "Pico de hierro" ;
        objetos[1] = "Espada de diamante" ;
        objetos[2] = "Hacha de hierro" ;
        objetos[3] = "Antorchas" ;
        objetos[4] = "Pan" ;

        for (int i = 0; i < objetos.length; i++){
            System.out.println( "Tienes en tu inventario : " + objetos[i] );
        }

        System.out.println("Total de objetos del jugador : " + objetos.length);
    }
}
